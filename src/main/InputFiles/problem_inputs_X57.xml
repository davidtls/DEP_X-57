<FASTOAD_model>
  <data>
    <TLAR>
      <NPAX_design is_input="True">2.0<!--design number of passengers (two pilots are included de facto, meaning for a 2 seater, NPAX_design is equal to 0)--></NPAX_design>
      <luggage_mass_design units="kg" is_input="True">0.0<!--luggage design mass--></luggage_mass_design>
      <range units="km" is_input="True">100.0<!--design range--></range>
      <v_approach units="m/s" is_input="True">39.0<!--approach speed--></v_approach>
      <v_cruise units="m/s" is_input="True">76.9<!--cruise speed--></v_cruise>
      <v_max_sl units="m/s" is_input="True">85.0<!--maximum speed at sea level--></v_max_sl>
    </TLAR>
    <geometry>
      <flap_type is_input="True">2.0<!--flap type (0.0 - plain flap, 2.0 - single slotted flap, 3.0 - split flap)--></flap_type>
      <has_T_tail is_input="True">0.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
      <wing_configuration is_input="True">3.0<!--1=low wing configuration / 2=mid wing configuration / 3=high wing configuration--></wing_configuration>
      <cabin>
        <aisle_width units="m" is_input="True">0.0<!--width of the aisle--></aisle_width>
        <luggage>
          <mass_max units="kg" is_input="True">60.0<!--maximum luggage mass--></mass_max>
        </luggage>
        <seats>
          <passenger>
            <NPAX_max is_input="True">2.0<!--maximum number of passengers in the aircraft--></NPAX_max>
            <count_by_row is_input="True">2.0<!--number of passenger seats per row--></count_by_row>
            <length units="m" is_input="True">0.9<!--passenger seats length--></length>
            <width units="m" is_input="True">0.4<!--width of passenger seats--></width>
          </passenger>
          <pilot>
            <length units="m" is_input="True">0.85<!--pilot seats length--></length>
            <width units="m" is_input="True">0.565<!--width of pilot seats--></width>
          </pilot>
        </seats>
      </cabin>
      <flap>
        <chord_ratio is_input="True">0.3<!--mean value of (flap chord)/(section chord)--></chord_ratio>
        <span_ratio is_input="True">0.7<!--flap span as a percentage of the wing span--></span_ratio>
      </flap>
      <horizontal_tail>
        <aspect_ratio is_input="True">6.499999999999999<!--horizontal tail aspect ratio--></aspect_ratio>
        <elevator_chord_ratio is_input="True">0.3<!--elevator chord ratio--></elevator_chord_ratio>
        <sweep_25 units="deg" is_input="True">10.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
        <taper_ratio is_input="True">0.6168398609570702<!--taper ratio of horizontal tail--></taper_ratio>
        <thickness_ratio is_input="True">0.1<!--thickness ratio of horizontal tail--></thickness_ratio>
        <MAC>
          <at25percent>
            <x>
              <from_wingMAC25 units="m" is_input="True">4.61<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
            </x>
          </at25percent>
        </MAC>
      </horizontal_tail>
      <landing_gear>
        <type is_input="True">1.0<!--0=non-retractable / 1=retractable--></type>
      </landing_gear>
      <propeller>
        <blade_pitch_angle units="deg" is_input="True">30.0</blade_pitch_angle>
        <blades_number is_input="True">3.0<!--number of blades on the propeller--></blades_number>
        <depth units="m" is_input="True">0.3<!--depth of the propeller--></depth>
        <diameter units="m" is_input="True">0.6743699999999992<!--propeller diameter--></diameter>
        <type is_input="True">1.0<!--1.0:fix pitch propeller, 2.0:variable pitch propeller--></type>
      </propeller>
      <vertical_tail>
        <aspect_ratio is_input="True">1.35<!--vertical tail aspect ratio--></aspect_ratio>
        <sweep_25 units="deg" is_input="True">30.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
        <taper_ratio is_input="True">0.5<!--taper ratio of vertical tail--></taper_ratio>
        <thickness_ratio is_input="True">0.1<!--thickness ratio of vertical tail--></thickness_ratio>
        <max_thickness>
          <x_ratio is_input="True">0.3<!--position of the point of maximum thickness as a ratio of vertical tail chord--></x_ratio>
        </max_thickness>
        <rudder>
          <chord_ratio is_input="True">0.3<!--flap rudder as a percentage of the wing chord--></chord_ratio>
          <max_deflection units="deg" is_input="True">30.0<!--rudder maximum deflection--></max_deflection>
        </rudder>
      </vertical_tail>
      <wing>
        <area units="m**2" is_input="True">8.258436629795337<!--wing area--></area>
        <aspect_ratio is_input="True">15.819105269216534<!--wing aspect ratio--></aspect_ratio>
        <dihedral units="rad" is_input="True">0.0</dihedral>
        <sweep_25 units="deg" is_input="True">2.0<!--sweep angle at 25% chord of wing--></sweep_25>
        <taper_ratio is_input="True">0.5<!--taper ratio of wing--></taper_ratio>
        <thickness_ratio is_input="True">0.12<!--mean thickness ratio of wing--></thickness_ratio>
        <twist units="rad" is_input="True">0.0<!--Negative twist means tip AOA is smaller than root--></twist>
        <kink>
          <span_ratio is_input="True">0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
        </kink>
      </wing>
      <electric_powertrain>
        <battery>
          <cell_diameter units="m" is_input="True">0.018</cell_diameter>
          <cell_length units="m" is_input="True">0.065</cell_length>
          <cell_mass units="kg" is_input="True">0.0445</cell_mass>
          <nb_packs is_input="True">2.0</nb_packs>
        </battery>
        <cables>
          <length units="m" is_input="True">20.0</length>
        </cables>
        <cooling_intake>
          <nb_intakes is_input="True">2.0</nb_intakes>
        </cooling_intake>
        <hex>
          <radiator_surface_density units="kg/m**2" is_input="True">40.0</radiator_surface_density>
        </hex>
      </electric_powertrain>
      <propulsion>
        <engine>
          <count is_input="True">12.0<!--number of engines--></count>
          <layout is_input="True">1.0<!--position of engines (1=under the wing / 2=rear fuselage)--></layout>
          <y_ratio is_input="True">[0.188, 0.3075, 0.4269, 0.5462, 0.667, 0.785]<!--engine position with respect to total span--></y_ratio>
        </engine>
      </propulsion>
    </geometry>
    <aerodynamics>
      <vertical_tail>
        <efficiency is_input="True">0.95</efficiency>
      </vertical_tail>
      <cooling>
        <cruise>
          <CD0 is_input="True">0.0005525<!--profile drag due to cooling in cruise conditions--></CD0>
        </cruise>
        <low_speed>
          <CD0 is_input="True">0.0005525<!--profile drag due to cooling in low speed conditions--></CD0>
        </low_speed>
      </cooling>
      <propeller>
        <cruise_level>
          <altitude units="m" is_input="True">1800.0<!--altitude at which the cruise level propeller efficiency map was computed--></altitude>
        </cruise_level>
        <installation_effect>
          <effective_efficiency>
            <high_speed is_input="True">1.0</high_speed>
          </effective_efficiency>
        </installation_effect>
      </propeller>
    </aerodynamics>
    <handling_qualities>
      <static_margin>
        <target is_input="True">0.05<!--static margin we want to achieve--></target>
      </static_margin>
    </handling_qualities>
    <mission>
      <sizing>
        <fuel units="kg" is_input="True">1.0</fuel>
        <max_motor_shaft_power_to_weight units="W/kg" is_input="True">8.374694208968629</max_motor_shaft_power_to_weight>
        <end_of_mission>
          <SOC is_input="True">0.05</SOC>
        </end_of_mission>
        <landing>
          <elevator_angle units="deg" is_input="True">-20.0<!--position of the elevator during landing--></elevator_angle>
          <flap_angle units="deg" is_input="True">30.0<!--position of the flaps during landing--></flap_angle>
          <target_sideslip units="deg" is_input="True">11.5</target_sideslip>
        </landing>
        <takeoff>
          <elevator_angle units="deg" is_input="True">10.0<!--position of the elevator during takeoff--></elevator_angle>
          <flap_angle units="deg" is_input="True">10.0<!--position of the flaps during takeoff--></flap_angle>
          <friction_coefficient_no_brake is_input="True">0.015<!--friction coefficient used during the computation of the takeoff phase--></friction_coefficient_no_brake>
          <thrust_rate is_input="True">1.0<!--thrust rate during takeoff phase--></thrust_rate>
        </takeoff>
        <taxi_in>
          <duration units="s" is_input="True">300.0<!--duration of taxi in phase--></duration>
          <offtakes units="W" is_input="True">1500.0</offtakes>
          <speed units="m/s" is_input="True">13.9<!--ground velocity during taxi in phase--></speed>
          <thrust_rate is_input="True">0.1<!--thrust rate during taxi in phase--></thrust_rate>
        </taxi_in>
        <taxi_out>
          <duration units="s" is_input="True">300.0<!--duration of taxi out phase--></duration>
          <offtakes units="W" is_input="True">1.0</offtakes>
          <speed units="m/s" is_input="True">13.9<!--ground velocity during taxi out phase--></speed>
          <thrust_rate is_input="True">0.1<!--thrust rate during taxi out phase--></thrust_rate>
        </taxi_out>
        <cs23>
          <characteristic_speed>
            <vd units="m/s" is_input="True">97.7<!--limit speed--></vd>
          </characteristic_speed>
          <sizing_factor>
            <ultimate_aircraft is_input="True">5.7<!--ultimate load factor that the aircraft will experience (default value is 5.7)--></ultimate_aircraft>
          </sizing_factor>
        </cs23>
        <main_route>
          <climb>
            <climb_thrust_rate is_input="True">1.0</climb_thrust_rate>
            <offtakes units="W" is_input="True">1000.0</offtakes>
            <climb_rate>
              <cruise_level units="m/s" is_input="True">4.0<!--target climb rate at the end of climb--></cruise_level>
              <sea_level units="m/s" is_input="True">4.5<!--target climb rate at sea level--></sea_level>
            </climb_rate>
          </climb>
          <cruise>
            <altitude units="m" is_input="True">1800.0<!--altitude during cruise phase in sizing mission--></altitude>
            <offtakes units="W" is_input="True">1500.0</offtakes>
          </cruise>
          <descent>
            <descent_rate units="m/s" is_input="True">-2.0<!--target descent rate for the aircraft--></descent_rate>
            <offtakes units="W" is_input="True">1500.0</offtakes>
          </descent>
          <reserve>
            <duration units="s" is_input="True">600.0<!--duration of the reserve segment--></duration>
          </reserve>
        </main_route>
      </sizing>
    </mission>
    <propulsion>
      <electric_powertrain>
        <battery>
          <cell_capacity units="A*h" is_input="True">3.0</cell_capacity>
          <cell_current_limit units="A" is_input="True">20.0</cell_current_limit>
          <cell_nom_voltage units="V" is_input="True">3.7</cell_nom_voltage>
          <int_resistance units="ohm" is_input="True">0.039</int_resistance>
          <max_C_rate units="1/h" is_input="True">6.7</max_C_rate>
          <sys_nom_voltage units="V" is_input="True">460.0</sys_nom_voltage>
          <type is_input="True">0.0<!--Optional type of battery - 0 is None and 1 is default Li-ion cells--></type>
        </battery>
        <cables>
          <lsw units="kg/m" is_input="True">0.5</lsw>
        </cables>
        <cooling_intake>
          <free_stream_speed units="m/s" is_input="True">28.0</free_stream_speed>
        </cooling_intake>
        <cores>
          <efficiency is_input="True">0.98</efficiency>
          <offtakes units="W" is_input="True">1500.0</offtakes>
          <power_density units="kW/L" is_input="True">10.0</power_density>
          <specific_power units="kW/kg" is_input="True">20.0</specific_power>
        </cores>
        <hex>
          <air_speed units="m/s" is_input="True">13.89</air_speed>
          <operating_temperature units="degK" is_input="True">293.0</operating_temperature>
        </hex>
        <inverter>
          <efficiency is_input="True">0.98</efficiency>
          <power_density units="kW/L" is_input="True">7.0</power_density>
          <specific_power units="kW/kg" is_input="True">20.0</specific_power>
        </inverter>
        <motor>
          <sizing_altitude units="m" is_input="True">1800.0</sizing_altitude>
          <sizing_delta_isa units="degC" is_input="True">20.0</sizing_delta_isa>
        </motor>
      </electric_powertrain>
    </propulsion>
    <weight>
      <airframe>
        <fuselage>
          <k_factor is_input="True">1.0<!--proportional corrective factor for fuselage mass--></k_factor>
        </fuselage>
        <horizontal_tail>
          <k_factor is_input="True">1.0<!--proportional corrective factor for horizontal tail mass--></k_factor>
        </horizontal_tail>
        <paint>
          <mass units="lbm" is_input="True">1.0<!--Mass of the airframe_inp_data:weight:airframe:paint:mass--></mass>
        </paint>
        <vertical_tail>
          <k_factor is_input="True">1.0<!--proportional corrective factor for vertical tail mass--></k_factor>
        </vertical_tail>
        <wing>
          <k_factor is_input="True">1.0<!--proportional corrective factor for wing mass--></k_factor>
        </wing>
      </airframe>
      <propulsion>
        <fuel_lines>
          <mass units="kg" is_input="True">1.0<!--fuel lines mass--></mass>
        </fuel_lines>
        <tank>
          <CG>
            <x units="m" is_input="True">1.0<!--X-position of center of gravity of the tank--></x>
          </CG>
        </tank>
      </propulsion>
    </weight>
  </data>
  <settings>
    <electrical_system>
      <SOC_in_reserve is_input="True">1.0</SOC_in_reserve>
    </electrical_system>
    <propulsion>
      <k_factor_psfc is_input="True">1.0</k_factor_psfc>
    </propulsion>
    <handling_qualities>
      <rudder>
        <safety_margin is_input="True">0.2<!--Ratio of the total rudder deflection not used in the computation of the VT area to leave a safety margin--></safety_margin>
      </rudder>
    </handling_qualities>
    <weight>
      <aircraft>
        <MLW_MZFW_ratio is_input="True">1.06</MLW_MZFW_ratio>
        <CG>
          <range is_input="True">0.3<!--distance between front position and aft position of CG, as ratio of mean aerodynamic chord (allows to have front position of CG potentially fwd of the computed one, as currently, FAST-OAD uses the aft position of CG as a reference)--></range>
          <aft>
            <MAC_position>
              <margin is_input="True">0.05<!--Added margin for getting most aft CG position, as ratio of mean aerodynamic chord--></margin>
            </MAC_position>
          </aft>
          <fwd>
            <MAC_position>
              <margin is_input="True">0.03<!--Added margin for getting most fwd CG position, as ratio of mean aerodynamic chord--></margin>
            </MAC_position>
          </fwd>
        </CG>
        <payload>
          <design_mass_per_passenger units="kg" is_input="True">80.0<!--Design value of mass per passenger--></design_mass_per_passenger>
          <max_mass_per_passenger units="kg" is_input="True">85.0<!--Maximum value of mass per passenger--></max_mass_per_passenger>
        </payload>
      </aircraft>
      <airframe>
        <landing_gear>
          <front>
            <front_fuselage_ratio is_input="True">0.15<!--Position of front landing gear expressed as fuselage front length ratio--></front_fuselage_ratio>
            <weight_ratio is_input="True">0.08<!--part of aircraft weight that is supported by front landing gear, should not be lower than 0.08. Roskam--></weight_ratio>
          </front>
        </landing_gear>
      </airframe>
    </weight>
    <aerodynamics>
      <aircraft>
        <undesirable_drag>
          <k_factor is_input="True">1.25<!--Correction coefficient to take into account the other undesirable drag, default is 1.25 as suggested in Gudmundsson--></k_factor>
        </undesirable_drag>
      </aircraft>
      <reference_flight_conditions>
        <cruise>
          <AOA units="rad" is_input="True">0.017453292519943295</AOA>
        </cruise>
        <low_speed>
          <AOA units="rad" is_input="True">0.08726646259971647</AOA>
        </low_speed>
      </reference_flight_conditions>
    </aerodynamics>
  </settings>
</FASTOAD_model>
